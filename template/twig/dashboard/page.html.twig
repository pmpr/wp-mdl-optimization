<div class="wrap">
    <h1 class="wp-heading-inline">{{ args.page_title|raw }}</h1>
    <hr class="wp-header-end">
    <div class="pr-panel-segment is-active is-full bg-transparent">
        <div class="row">
            <div class="col-12 col-md-4 col-lg-3 mb-3">

                {% set quick_actions_html %}
                    {% for action in quick_actions %}
                        <div class="{% if not loop.last %}mb-3{% endif %}">
                            {{ action|raw }}
                        </div>
                    {% endfor %}
                {% endset %}

                {{ render_backend_card({
                    header: quick_action_title,
                    content: [quick_actions_html],
                    card_class: 'mb-4'
                }) }}

                {{ render_backend_card({
                    header: subscription.title,
                    content: subscription.table,
                }) }}
            </div>
            <div class="col-12 col-md-8 col-lg-9">
                {{ render_backend_card({
                    header: report.title,
                    content: report.panel,
                }) }}
            </div>
        </div>
    </div>
</div>
